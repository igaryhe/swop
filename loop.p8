-- util
function coroutine()
if cor and costatus(cor)!='dead' then
 	coresume(cor)
 else
 	cor=nil
 end
end

function win(pl)
	if fget(mget(cam.x+pl.x,cam.y+pl.y),7) then
		cam.x=level%8*16
		cam.y=flr(level/8)*16
		cmd_list={}
		level+=1
		if(level==9) mode=2
	end
end
